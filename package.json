{
  "name": "nextbase",
  "version": "1.0.0",
  "main": "lib/index.js",
  "engines": {
    "node": "14"
  },
  "scripts": {
    "dev": "next dev",
    "sitemap": "next-sitemap",
    "start": "next start",
    "style": "tsc && npm run style:lint && npm run style:prettier",
    "style:lint": "eslint '**/*.{ts,tsx}'",
    "style:prettier": "prettier --check '**/*.{ts,tsx}'",
    "make-pretty": "prettier --write '**/*.{ts,tsx}'",
    "build:next": "npm run style && next build && npm run sitemap",
    "build:cloud": "tsc --build server.tsconfig.json",
    "build": "npm run build:next && npm run build:cloud",
    "serve": "firebase serve"
  },
  "dependencies": {
    "@headlessui/react": "^1.4.0",
    "@heroicons/react": "^1.0.4",
    "firebase": "^9.0.0-beta.8",
    "firebase-admin": "^9.11.1",
    "firebase-functions": "^3.15.4",
    "next": "^11.1.0",
    "next-pwa": "^5.3.1",
    "nprogress": "^0.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-toast-notifications": "^2.5.1",
    "tailwindcss": "^2.2.7"
  },
  "devDependencies": {
    "@types/node": "^16.7.1",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "autoprefixer": "^10.3.2",
    "eslint": "^7.32.0",
    "eslint-config-next": "^11.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "next-sitemap": "^1.6.162",
    "postcss": "^8.3.6",
    "prettier": "^2.3.2",
    "sass": "^1.38.0",
    "typescript": "^4.3.5",
    "webpack": "^5.51.1"
  }
}
